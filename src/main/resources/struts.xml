<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
	<constant name="struts.action.excludePattern" value="/soap.*" />
	<constant name="struts.ui.theme" value="simple" />
	
	<package name="health-hub" extends="struts-default">
        <action name="">
            <result>/WEB-INF/jsp/index.jsp</result>
        </action>
        <action name="register" class="br.com.bryan.actions.RegisterAction">
        	<result name="success">/WEB-INF/jsp/index.jsp</result>
            <result name="input">/WEB-INF/jsp/index.jsp</result>
            <result name="error">/WEB-INF/jsp/index.jsp</result>
        </action>
        <action name="loginAction" class="br.com.bryan.actions.LoginAction">
	        <result name="success">/WEB-INF/jsp/index.jsp</result>
	        <result name="input">/WEB-INF/jsp/index.jsp</result>
	        <result name="error">/WEB-INF/jsp/index.jsp</result>
	    </action>
	    <action name="logout" class="br.com.bryan.actions.LogoutAction">
	        <result name="success">/WEB-INF/jsp/index.jsp</result>
	    </action>
	    <action name="exam">
	        <result>/WEB-INF/jsp/exam/exam.jsp</result>
	    </action>
	    <action name="editExam">
	        <result>/WEB-INF/jsp/exam/editExam.jsp</result>
	    </action>
	    <action name="newExam">
	        <result>/WEB-INF/jsp/exam/newExam.jsp</result>
	    </action>
	</package>
</struts>